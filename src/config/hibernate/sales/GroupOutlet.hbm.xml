<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.wo.epos.module.sales.groupOutlet.model">
	<class name="GroupOutlet" table="POS_GROUP_OUTLET">
		<id name="groupOutletId" column="POS_GROUP_OUTLET_ID" type="long">
			<generator class="native">
				<param name="sequence">GROUP_OUTLET_SEQ</param>
			</generator>
		</id>

		<property name="groupOutletCode" column="POS_GROUP_OUTLET_CODE" type="string" />
		<property name="groupOutletName" column="POS_GROUP_OUTLET_NAME" type="string" />
		        
		<property name="activeFlag" column="ACTIVE_FLAG" type="string" />
		<property name="createOn" column="CREATE_ON" type="timestamp" />
		<property name="createBy" column="CREATE_BY" type="string" />
		<property name="updateOn" column="UPDATE_ON" type="timestamp" />
		<property name="updateBy" column="UPDATE_BY" type="string" />
			
	</class>
</hibernate-mapping>
